<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<!-- 修改学校名称 -->
<suite name="集美大学诚毅学院视频播放" parallel="tests" preserve-order="true"
	thread-count="1">
	<listeners>
		<listener class-name="com.kkb.test.util.reporter.HTMLReporter" />
		<listener class-name="com.kkb.test.util.reporter.JUnitXMLReporter" />
		<listener class-name="com.kkb.test.util.reporter.TestngListener" />
	</listeners>
	
	
	<!-- 修改下面三处，依次为学校首页，用户名 密码 -->
	<parameter name="baseUrl" value="http://chengyi.gaoxiaobang.com/" />
	<parameter name="loginName" value="gxb0000" />
	<parameter name="password" value="1234567" />



	<!-- 课程名称1 按照下列格式，修改2处课程名称即可，例如修改为‘计算机文化基础【诚毅学院】’， -->
	<test name="计算机文化基础【诚毅学院】">
		<parameter name="courseName" value="计算机文化基础【诚毅学院】" />
		<classes>
			<class name="com.kkb.test.steps.pc.gxb.CoursePlayStep" />
		</classes>
	</test>
	
    <!-- 课程名称2 后面依次类推 ，添加课程复制即可-->
	<test name="口才与人际交往【诚毅学院】">
		<parameter name="courseName" value="口才与人际交往【诚毅学院】" />
		<classes>
			<class name="com.kkb.test.steps.pc.gxb.CoursePlayStep" />
		</classes>
	</test>

	

</suite>


